<resources>

    <!-- Base application theme. -->
    <style name="module_common_AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/module_common_colorPrimary</item>
        <item name="colorPrimaryDark">@color/module_common_colorPrimaryDark</item>
        <item name="colorAccent">@color/module_common_colorAccent</item>
    </style>

    <style name="module_common_AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="module_common_AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="module_common_AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
